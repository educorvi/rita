{
    "name": "@educorvi/persistent-rita",
    "description": "This is a package to persist rita rulesets in a database",
    "version": "6.0.3",
    "repository": {
        "type": "git",
        "url": "https://github.com/educorvi/rita",
        "directory": "persistent-rita"
    },
    "license": "MIT",
    "author": "Julian Pollinger",
    "sideEffects": false,
    "type": "module",
    "main": "./dist/persistent-rita.cjs",
    "module": "./dist/persistent-rita.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/persistent-rita.mjs",
            "require": "./dist/persistent-rita.cjs",
            "default": "./dist/persistent-rita.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "type-check": "tsc --noEmit",
        "prebuild": "rimraf dist",
        "build": "vite build",
        "doc": "npx typedoc",
        "prepack": "yarn run build",
        "test": "jest 2>&1",
        "semantic-release": "semantic-release"
    },
    "dependencies": {
        "reflect-metadata": "^0.2.2"
    },
    "devDependencies": {
        "@educorvi/rita": "*",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.0.10",
        "dotenv": "^17.0.1",
        "jest": "^30.0.4",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.4.0",
        "tslib": "^2.8.1",
        "typedoc": "0.28.7",
        "typeorm": "^0.3.25",
        "typescript": "5.8.3",
        "vite": "^6.0.5",
        "vite-plugin-dts": "^4.4.0"
    },
    "peerDependencies": {
        "@educorvi/rita": "^5.4.4",
        "mysql2": "^3.6.5",
        "pg": "^8.7.1",
        "sqlite3": "^5.0.6",
        "typeorm": "^0.3.4"
    },
    "engines": {
        "node": ">=14"
    },
    "publishConfig": {
        "access": "public"
    }
}
