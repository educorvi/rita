{
    "name": "@educorvi/rita",
    "version": "5.4.4",
    "description": "JSON Schema for rules that can be used to evaluate data and a Typescript implementation to evaluate those rules",
    "bugs": {
        "url": "https://github.com/educorvi/rita/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/educorvi/rita.git",
        "directory": "rita-core"
    },
    "license": "MIT",
    "author": "Julian Pollinger",
    "sideEffects": false,
    "type": "module",
    "main": "./dist/rita-core.cjs",
    "module": "./dist/rita-core.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/rita-core.mjs",
            "require": "./dist/rita-core.cjs",
            "default": "./dist/rita-core.js"
        }
    },
    "files": [
        "dist",
        "src",
        "src/schema"
    ],
    "scripts": {
        "type-check": "tsc --noEmit",
        "build": "yarn run clean && vite build",
        "clean": "rimraf dist dist_zip",
        "doc": "yarn run doc:implementation && yarn run doc:schema && git add docs",
        "doc:implementation": "npx typedoc --out docs/implementation src/index.ts",
        "doc:schema": "jsonschema2md -d src/schema -o docs/schema -e json -x -",
        "prepack": "yarn run build",
        "test": "jest 2>&1",
        "semantic-release": "semantic-release"
    },
    "dependencies": {
        "ajv": "^8.17.1",
        "ajv-formats": "^2.1.1",
        "luxon": "^3.6.1",
        "tslib": "^2.8.1"
    },
    "devDependencies": {
        "@adobe/jsonschema2md": "^8.0.3",
        "@types/deep-equal": "^1.0.4",
        "@types/jest": "^30.0.0",
        "@types/luxon": "^3.6.2",
        "@types/madge": "^5.0.3",
        "@types/node": "^24.0.10",
        "jest": "^30.0.4",
        "jest-extended": "^6.0.0",
        "madge": "^8.0.0",
        "mkdirp": "^3.0.1",
        "rimraf": "^6.0.1",
        "typedoc": "0.28.7",
        "typescript": "5.8.3",
        "vite": "^6.0.5",
        "vite-plugin-dts": "^4.4.0"
    },
    "engines": {
        "node": ">=10"
    },
    "publishConfig": {
        "access": "public"
    }
}
