{
    "$schema": "https://raw.githubusercontent.com/educorvi/rita/develop/rita-core/src/schema/schema.json",
    "rules": [
        {
            "id": "satisfiable",
            "rule": {
                "type": "comparison",
                "operation": "equal",
                "arguments": [
                    "testValue",
                    {
                        "type": "atom",
                        "path": "testPath"
                    }
                ]
            }
        },
        {
            "id": "string comparison",
            "rule": {
                "type": "comparison",
                "operation": "smaller",
                "arguments": [
                    {
                        "type": "atom",
                        "path": "randomName"
                    },
                    "Julian"
                ]
            }
        },
        {
            "id": "string comparison2",
            "rule": {
                "type": "comparison",
                "operation": "greater",
                "arguments": [
                    {
                        "type": "atom",
                        "path": "randomName2"
                    },
                    "Julian"
                ]
            }
        }
    ]
}
