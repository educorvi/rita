{
  "name": "@educorvi/rita-smt",
  "version": "2.1.0",
  "description": "A module to test satisfiability of Rita rule sets.",
  "repository": "https://github.com/educorvi/rita",
  "license": "MIT",
  "author": "Julian Pollinger",
  "main": "dist/rita-smt.js",
  "module": "dist/rita-smt.mjs",
  "typings": "dist/index.d.ts",
  "bin": "bin/rita-smt-bin.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "build:internal": "vite build && vite build --config vite.bin.config.ts",
    "build": "turbo build:internal check-types",
    "prepack": "npm run build",
    "runbin": "ts-node src/bin.ts",
    "test:manual": "vitest run",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "@educorvi/smtlib": "workspace:^1.3.0",
    "command-exists": "^1.2.9",
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@educorvi/rita": "workspace:^5.5.0",
    "@semantic-release/exec": "^7.1.0",
    "@types/command-exists": "^1.2.3",
    "@types/node": "^24.0.10",
    "@types/terminal-kit": "^2.5.7",
    "semantic-release": "^25.0.2",
    "semantic-release-monorepo": "^8.0.2",
    "semantic-release-yarn": "^3.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "5.8.3",
    "unplugin-dts": "~1.0.0-beta.6",
    "vite": "~7.2.4",
    "vite-plugin-externalize-deps": "~0.10.0",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "@educorvi/rita": "5.5.0"
  }
