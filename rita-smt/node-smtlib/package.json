{
  "name": "@educorvi/smtlib",
  "version": "1.1.1",
  "description": "A Javascript wrapper with a Z3-ish API for SMT-LIB 2.0, the language of Satisfiability Modulo Theories solvers",
  "author": "Julian Pollinger",
  "repository": "https://github.com/educorvi/rita-smt",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "dependencies": {
    "@types/byline": "^4.2.33",
    "byline": "^5.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./lib ./test",
    "test": "nyc ts-node ./test/test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "release": "yarn install && semantic-release -e semantic-release-monorepo",
    "prepack": "npm run build"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript"
  }
}
