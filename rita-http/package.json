{
    "name": "rita-http",
    "version": "5.1.0",
    "description": "HTTP API for Rita",
    "keywords": [
        "rita",
        "express.js",
        "typescript",
        "api"
    ],
    "homepage": "https://github.com/educorvi/rita/tree/develop/rita-http#readme",
    "bugs": {
        "url": "https://github.com/educorvi/rita/tree/develop/rita-http/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/educorvi/rita.git"
    },
    "license": "MIT",
    "author": "Julian Pollinger",
    "main": "dist/rita-http.cjs",
    "bin": {
        "rita-http": "dist/rita-http.cjs"
    },
    "type": "module",
    "files": [
        "dist"
    ],
    "scripts": {
        "_phase:build": "rushx build",
        "_phase:test": "rushx test",
        "check-types": "tsc --noEmit",
        "prepack": "npm run build",
        "build": "npm run generate-routes && npm run spec && vite build",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "dev:config": "ts-node-dev --respawn --transpile-only src/index.ts -- --config",
        "doc": "npm run spec",
        "generate-routes": "npx tsoa routes",
        "spec": "npx tsoa spec",
        "start": "node .",
        "test:no-server": "LOGLEVEL=error vitest run",
        "test": "start-server-and-test start http://localhost:3000 test:no-server"
    },
    "dependencies": {
        "@educorvi/persistent-rita": "workspace:^7.0.0",
        "@educorvi/rita": "workspace:^5.5.0",
        "@educorvi/rita-plugin-http": "workspace:*",
        "@tsoa/runtime": "^6.6.0",
        "body-parser": "^2.2.0",
        "dotenv": "17.2.3",
        "express": "^4.21.2",
        "pg": "~8.16.3",
        "promise.any": "^2.0.6",
        "reflect-metadata": "^0.2.2",
        "semver": "^7.7.2",
        "sqlite3": "^5.1.7",
        "swagger-ui-express": "^5.0.1",
        "terminal-kit": "^3.1.2",
        "tslog": "^4.9.3",
        "tsoa": "^6.6.0",
        "typeorm": "0.3.27",
        "mysql2": "^3.14.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@types/body-parser": "^1.19.6",
        "@types/express": "^4.17.23",
        "@types/node": "^24.0.10",
        "@types/semver": "^7.7.0",
        "@types/swagger-ui-express": "^4.1.8",
        "@types/terminal-kit": "^2.5.7",
        "axios": "^1.10.0",
        "bestzip": "^2.2.1",
        "start-server-and-test": "^2.0.12",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "5.8.3",
        "vite": "~7.2.4",
        "vite-plugin-externalize-deps": "~0.10.0",
        "unplugin-dts": "~1.0.0-beta.6",
        "@rollup/plugin-swc": "~0.4.0",
        "@swc/core": "~1.15.3",
        "vitest": "^2.1.9"
    }
}
