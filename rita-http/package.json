{
    "name": "rita-http",
    "version": "5.0.1",
    "description": "HTTP API for Rita",
    "keywords": [
        "rita",
        "express.js",
        "typescript",
        "api"
    ],
    "homepage": "https://github.com/educorvi/rita/tree/develop/rita-http#readme",
    "bugs": {
        "url": "https://github.com/educorvi/rita/tree/develop/rita-http/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/educorvi/rita.git",
        "directory": "rita-http"
    },
    "license": "MIT",
    "author": "Julian Pollinger",
    "main": "dist/ritaAPI.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "type-check": "tsc --noEmit",
        "prepack": "yarn run build",
        "build": "yarn run generate-routes && yarn run spec && webpack",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "dev:config": "ts-node-dev --respawn --transpile-only src/index.ts -- --config",
        "doc": "yarn run spec",
        "generate-routes": "npx tsoa routes",
        "spec": "npx tsoa spec",
        "start": "node .",
        "test:no-server": "LOGLEVEL=error jest",
        "test": "start-server-and-test start http://localhost:3000 test:no-server"
    },
    "dependencies": {
        "@educorvi/persistent-rita": "*",
        "@educorvi/rita": "*",
        "@educorvi/rita-plugin-http": "*",
        "@tsoa/runtime": "^6.6.0",
        "body-parser": "^2.2.0",
        "dotenv": "^17.0.1",
        "express": "^4.21.2",
        "pg": "~8.16.3",
        "promise.any": "^2.0.6",
        "reflect-metadata": "^0.2.2",
        "semver": "^7.7.2",
        "sqlite3": "^5.1.7",
        "swagger-ui-express": "^5.0.1",
        "terminal-kit": "^3.1.2",
        "tslog": "^4.9.3",
        "tsoa": "^6.6.0",
        "typeorm": "^0.3.25",
        "mysql2": "^3.14.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@types/body-parser": "^1.19.6",
        "@types/chai": "^5.2.2",
        "@types/express": "^4.17.23",
        "@types/mocha": "^10.0.10",
        "@types/node": "^24.0.10",
        "@types/semver": "^7.7.0",
        "@types/swagger-ui-express": "^4.1.8",
        "@types/terminal-kit": "^2.5.7",
        "axios": "^1.10.0",
        "bestzip": "^2.2.1",
        "chai": "^5.2.0",
        "start-server-and-test": "^2.0.12",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "5.8.3",
        "webpack": "^5.99.9",
        "webpack-cli": "^6.0.1",
        "webpack-node-externals": "^3.0.0",
        "jest": "^30.0.4",
        "ts-jest": "^29.4.0",
        "@types/jest": "^30.0.0"
    }
}
